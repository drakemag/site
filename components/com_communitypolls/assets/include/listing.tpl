<div class="ui-widget ui-widget-content poll-item {lastitemcss}">
	<div class="poll-avatar">{avatar}</div>
	<div class="ui-widget ui-widget-content {highlightcss} poll-votes">
		<div class="poll-votes-inset">
			<div class="vote-count">{votes}</div>
			<div class="vote-meta">{votestext}</div>
		</div>
	</div>
	<div class="poll-title"><a href="{pollurl}">{polltitle}</a></div>
	<div class="poll-meta">{pollmetatext}</div>
	<div class="float-clear"></div>
</div>