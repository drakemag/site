<div class="poll-template">
	<div class="poll-title">{title}</div>
	<div class="poll-details">
		<div class="poll-description">{description}</div>
	</div>
	<div class="poll-notices">{notices}</div>
	<div class="poll-chart">{chart}</div>
	<div class="poll-meta">
		<div>Votes: {votes}</div>
		<div>Last vote: {lastvote}</div>
	</div>
	<div class="poll-navigation buttons">{actions}<div class="poll-clear"></div></div>
</div>